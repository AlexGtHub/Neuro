        "#Turing"
      ],
      "metadata": {
        "id": "9-R5JmiIRYfF"
      },
      "id": "9-R5JmiIRYfF"
    },
    {
      "cell_type": "code",
      "source": [
        "from matplotlib import pyplot as plt\n",
        "from PIL import Image\n",
        "import numpy as np\n",
        "#1 blanco -1 negro\n",
        "img1 = Image.open('einstein.png')\n",
        "img2 = Image.open('cocodrilo.png')\n",
        "img3 = Image.open(\"turing.jpg\")\n",
        "\n",
        "new_h = 64\n",
        "new_i = 64\n",
        "\n",
        "img1_small = img1.resize((new_h,new_i))\n",
        "img2_small = img2.resize((new_h,new_i))\n",
        "img3_small = img3.resize((new_h,new_i))\n",
        "\n",
        "p1 = np.asarray(img1_small)[:,:,0]>150\n",
        "p2 = np.asarray(img2_small)[:,:,0]>150\n",
        "p3 = np.asarray(img3_small)[:,:,0]>150\n",
        "\n",
        "p1 = 2*p1-1\n",
        "p2 = 2*p2-1\n",
        "p3 = 2*p3-1\n",
        "\n",
        "#plt.imshow(p1,cmap='Greys_r')\n",
        "#plt.imshow(p2, cmap=\"Greys_r\")\n",
        "\n",
        "p1_flat = p1.flatten()\n",
        "p2_flat = p2.flatten()\n",
        "p3_flat = p3.flatten()\n",
        "\n",
        "N = 64**2\n",
        "\n",
        "W = np.zeros([N,N])\n",
        "\n",
        "estado_inicialp = p3[20:40, 20:40] #saco el bloque 20x20\n",
        "\n",
        "S = np.ones([64,64]) #creo matriz blanca de tamaño\n",
        "\n",
        "S[20:40, 20:40] = estado_inicialp # inserto bloque en matriz blanca\n",
        "\n",
        "bloque = S\n",
        "\n",
        "S = S.flatten()\n",
        "\n",
        "#alamacenar patrones\n",
        "for i in range(N):\n",
        "    for j in range(N):\n",
        "        W[i, j] += (1/N) * ((p1_flat[i] * p1_flat[j]) + (p2_flat[i] * p2_flat[j]) + (p3_flat[i] * p3_flat[j]))\n",
        "\n",
        "#signo\n",
        "def sgn(x):\n",
        "    return 1 if x >= 0 else -1\n",
        "\n",
        "\n",
        "#regla de actualizacion\n",
        "for i in range(N):\n",
        "    idx = 0\n",
        "    for j in range(N):\n",
        "        idx += W[i,j] * S[j]\n",
        "    S[i] = sgn(idx)\n",
        "\n",
        "overlap_p1 = []\n",
        "overlap_p2 = []\n",
        "overlap_p3 = []\n",
        "\n",
        "m1 = 0\n",
        "m2 = 0\n",
        "m3 = 0\n",
        "for i in range(N):\n",
        "    m1 += (1/N)*(p1_flat[i] * S[i])\n",
        "    m2 += (1/N)*(p2_flat[i] * S[i])\n",
        "    m3 += (1/N)*(p3_flat[i] * S[i])\n",
        "    overlap_p1.append(m1)\n",
        "    overlap_p2.append(m2)\n",
        "    overlap_p3.append(m3)\n",
        "\n",
        "print(f\"Einstein: {m1}, Cocodrilo: {m2}, Turing: {m3}\")\n",
        "\n",
        "#volver al 64x64\n",
        "S = S.reshape((64, 64))\n",
        "\n",
        "fig, axs = plt.subplots(2, 3, figsize=(10, 8))\n",
        "\n",
        "axs[0, 0].imshow(bloque, cmap=\"Greys_r\")\n",
        "axs[0, 0].set_title(\"Bloque 20x20\")\n",
        "axs[0, 0].axis('off')\n",
        "\n",
        "axs[0, 1].imshow(S, cmap=\"Greys_r\")\n",
        "axs[0, 1].set_title(\"Patron encontrado\")\n",
        "axs[0, 1].axis('off')\n",
        "\n",
        "axs[0,2].axis(\"off\")\n",
        "\n",
        "axs[1, 0].plot(overlap_p1)\n",
        "axs[1, 0].set_title(f\"Overlap Einstein = {m1}\")\n",
        "\n",
        "axs[1, 1].plot(overlap_p2)\n",
        "axs[1, 1].set_title(f\"Overlap Cocodrilo = {m2}\")\n",
        "\n",
        "axs[1, 2].plot(overlap_p3)\n",
        "axs[1, 2].set_title(f\"Overlap Turing = {m3}\")\n",
        "\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()"
